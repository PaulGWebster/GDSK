  withBase = withBase === 'true';
  const data = common.bakeUrlData(type, e, withBase, true);
  switch (prop) {
    case 'protocol':
    case 'username':
    case 'password':
    case 'host':
    case 'hostname':
    case 'port':
    case 'pathname':
    case 'search':
    case 'hash':
    case 'href':
      setAndGet(data, prop);
      break;
    case 'origin':
    case 'searchParams':
      get(data, prop);
      break;
    default:
      throw new Error('Unknown prop');
  }
