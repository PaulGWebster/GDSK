#define EXPORT_FUNC(env, exports, name, func)                                  \
  do {                                                                         \
    napi_value js_func;                                                        \
    NAPI_CALL(napi_create_function(                                            \
        (env), (name), NAPI_AUTO_LENGTH, (func), NULL, &js_func));             \
