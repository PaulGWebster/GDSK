  for (let i = 0; i < n; i++) {
    for (let j = 0; j < files.length; j++) {
      try {
        const dir = fs.opendirSync(files[j]);
        dir.closeSync();
      } catch {
        // do nothing
      }
    }
  }
  bench.end(n);
}
