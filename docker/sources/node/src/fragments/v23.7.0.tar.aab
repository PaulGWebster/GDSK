    );

    socket.write(frame);
  });

  server.listen(0, () => {
    const ws = new WebSocket(`ws://localhost:${server.address().port}`);
    ws.addEventListener('open', () => {
      bench.start();
    });

    ws.addEventListener('message', (event) => {
      ws.send(event.data);
    });
