    const client = http2.connect(`http://localhost:${server.address().port}/`, {
      maxHeaderListPairs: 20000,
    });

    function doRequest(remaining) {
      const req = client.request(headersObject);
      req.resume();
      req.on('end', () => {
        if (remaining > 0) {
