                                       GTEST_STRINGIFY_(CaseName),           \
                                       GTEST_STRINGIFY_(TestName), 0,        \
                                       ::testing::internal::GenerateNames<   \
                                           GTEST_NAME_GENERATOR_(CaseName),  \
                                           GTEST_TYPE_PARAMS_(CaseName)>()); \
