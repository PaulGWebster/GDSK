inline char16_t
UnicodeString::doCharAt(int32_t offset) const
{
  if (static_cast<uint32_t>(offset) < static_cast<uint32_t>(length())) {
    return getArrayStart()[offset];
  } else {
    return kInvalidUChar;
  }
}

