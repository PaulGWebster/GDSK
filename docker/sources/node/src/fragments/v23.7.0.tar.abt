    ParseResourceFunction *parseFunction;
} gResourceTypes[] = {
    {"Unknown", nullptr, nullptr},
    {"string", k_type_string, parseString},
    {"binary", k_type_binary, parseBinary},
    {"table", k_type_table, parseTable},
