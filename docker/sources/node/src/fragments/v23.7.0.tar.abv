#define JSON_CATCH_USER(exception) if(false)
#define JSON_THROW_USER(exception) { \
    printf("JSON exception thrown; modify tools/toolutil/ujson.h to get diagnostics.\n\
Exiting immediately.\n"); \
    exit(1); \
}
#endif

