    defines = [ "_U_" ]
    if (is_win) {
      defines += [
        "WIN32",
        "_WINDOWS",
        "HAVE_CONFIG_H",
      ]
    } else {
      defines += [
        "HAVE_UNISTD_H",
      ]
    }
    if (is_linux) {
      defines += [
        "HAVE_ARPA_INET_H",
        "HAVE_NETINET_IN_H",
      ]
    }

    include_dirs = [
      ".",
      "ngtcp2/lib/",
      "ngtcp2/crypto/",
      "nghttp3/lib/"
    ]

    sources = gypi_values.nghttp3_sources +
              gypi_values.ngtcp2_sources +
              gypi_values.ngtcp2_sources_quictls
    if (node_use_openssl) {
      deps = [ "../openssl" ]
    }

    if (is_clang || !is_win) {
      cflags_c = [
        "-Wno-extra-semi",
        "-Wno-implicit-fallthrough",
        "-Wno-unused-function",
      ]
    }
  }
}
