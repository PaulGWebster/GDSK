#include <ngtcp2/ngtcp2.h>

typedef struct ngtcp2_opl_entry ngtcp2_opl_entry;

struct ngtcp2_opl_entry {
  ngtcp2_opl_entry *next;
};

/*
 * ngtcp2_opl is an object memory pool.
 */
typedef struct ngtcp2_opl {
  ngtcp2_opl_entry *head;
} ngtcp2_opl;

