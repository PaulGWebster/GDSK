let module = (() => {
  let builder = new WasmModuleBuilder();
  builder.addMemory(1, undefined);
  builder.addFunction("grow_memory", kSig_i_i)
              .addBody([kExprLocalGet, 0, kExprMemoryGrow, kMemoryZero])
