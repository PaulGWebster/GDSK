  let array = new Int32Array(buffer);
  let dataview = new DataView(buffer);

  array[0] = 123;
  assertEquals(123, instance.exports.getInt32(dataview, 0, 1));
  instance.exports.setInt32(dataview, 4, 100, 1);
  assertEquals(100, array[1]);
