builder.addFunction("test", kSig_v_v).exportFunc().addBody([
  kExprRefNull, kNullRefCode,
  kExprI32Const, 0,
  kExprI32Const, 0,
  ...GCInstr(kExprStringNewWtf16Array),
  kExprDrop,
]);

builder.addFunction("test2", kSig_v_v).exportFunc().addBody([
  kExprRefNull, kNullRefCode,
  kExprRefAsNonNull,
  kExprI32Const, 0,
  kExprI32Const, 0,
  ...GCInstr(kExprStringNewWtf16Array),
  kExprDrop,
