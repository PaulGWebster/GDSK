});

test('subtesting', (t) => {
  t.plan(1);
  t.test('subtest', () => { });
});

test('subtesting correctly', (t) => {
  t.plan(2);
  t.assert.ok(true);
  t.test('subtest', (st) => {
    st.plan(1);
    st.assert.ok(true);
  });
});

test('correctly ignoring subtesting plan', (t) => {
