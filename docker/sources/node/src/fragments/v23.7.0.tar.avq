'use strict';
require('../common');
const assert = require('assert');
const vm = require('vm');

// Timeout of 100ms executing endless loop
assert.throws(
  function() {
    vm.runInThisContext('while(true) {}', { timeout: 100 });
  },
  {
    code: 'ERR_SCRIPT_EXECUTION_TIMEOUT',
    message: 'Script execution timed out after 100ms'
