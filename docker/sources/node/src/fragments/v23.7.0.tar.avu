const res = await fetch('https://api.github.com/repos/nghttp2/nghttp2/releases/latest',
  process.env.GITHUB_TOKEN && {
    headers: {
      "Authorization": `Bearer ${process.env.GITHUB_TOKEN}`
    },
  });
